{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "src": "/swagger/(.*)",
      "dest": "/public/swagger/$1"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api-docs",
      "dest": "/server.js"
    },
    {
      "src": "/swagger.json",
      "dest": "/server.js"
    }
  ]
}