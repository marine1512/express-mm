{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "rewrites": [
    { "source": "/api-docs-static/(.*)", "destination": "/api-docs-static/$1" },
    { "source": "/api-docs/(.*)", "destination": "/api-docs/$1" }
  ]
}